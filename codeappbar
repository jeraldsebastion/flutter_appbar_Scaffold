import 'package:flutter/material.dart';

void main(){
final barcolor = const Color(0xFF2C3335);
final bgcolor = const Color(0xFFEAF0F1);
  var app=MaterialApp(
    home: Scaffold(
     backgroundColor: bgcolor,
      appBar: AppBar(
     backgroundColor:barcolor ,
      ),
    ),
  );
runApp(app);
}
